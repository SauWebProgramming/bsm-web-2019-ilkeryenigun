    Layout = "~/Areas/Admin/Views/Shared/_AdminLayout.cshtml";
}

<section id="container">
    <section id="main-content">
        <section class="wrapper">
            <div class="col-lg-12">
                <section class="panel">

                    <div class="panel-body">
                        @using (Html.BeginForm("RolEkle", "RolYonetim", FormMethod.Post))
                        {
                            @Html.TextBoxFor(m => m.RolAd)
                            <input type="submit" />
                        }
                        </div>
                </section>
                </div>
            </section>
        </section>
    </section>

